{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "databrick-course-df"
		},
		"ls_covid19reportinglksc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_covid19reportinglksc'"
		},
		"ls_covidstoragesac_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covidstoragesac'"
		},
		"ls_formula1dl_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_formula1dl_storage'"
		},
		"ls_covid19reportinglksc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid19reportinglksc.dfs.core.windows.net/"
		},
		"ls_databricks_course_ws_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0225-050922-hmxerc5t"
		},
		"ls_formula1dl_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formula1nd.dfs.core.windows.net/"
		},
		"ls_opendate_ecdc_europe_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.ecdc.europa.eu"
		},
		"tr_population_ingest_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0623c456-13a8-47ab-a9c0-018dc750d42e/resourceGroups/databricks-rs/providers/Microsoft.Storage/storageAccounts/covidstoragesac"
		},
		"tr_transform_formula1_data_properties_pipeline_parameters_p_window_end_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"tr_ingest_hospital_admission_data_properties_pl_ecdc_death_cases_parameters_servicerelativeURL": {
			"type": "string",
			"defaultValue": "covid19/hospitalicuadmissionrates/csv/data.csv"
		},
		"tr_ingest_hospital_admission_data_properties_pl_ecdc_death_cases_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admission.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "databrick-course-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_covid19reportinglksc_accountKey": {
						"value": "[parameters('ls_covid19reportinglksc_accountKey')]"
					},
					"ls_covidstoragesac_connectionString": {
						"value": "[parameters('ls_covidstoragesac_connectionString')]"
					},
					"ls_formula1dl_storage_accountKey": {
						"value": "[parameters('ls_formula1dl_storage_accountKey')]"
					},
					"ls_covid19reportinglksc_properties_typeProperties_url": {
						"value": "[parameters('ls_covid19reportinglksc_properties_typeProperties_url')]"
					},
					"ls_databricks_course_ws_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_course_ws_properties_typeProperties_existingClusterId')]"
					},
					"ls_formula1dl_storage_properties_typeProperties_url": {
						"value": "[parameters('ls_formula1dl_storage_properties_typeProperties_url')]"
					},
					"ls_opendate_ecdc_europe_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_opendate_ecdc_europe_eu_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "databrick-course-df_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_ingest_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_ingest_data_properties_typeProperties_scope')]"
					},
					"tr_ingest_hospital_admission_data_properties_pl_ecdc_death_cases_parameters_servicerelativeURL": {
						"value": "[parameters('tr_ingest_hospital_admission_data_properties_pl_ecdc_death_cases_parameters_servicerelativeURL')]"
					},
					"tr_ingest_hospital_admission_data_properties_pl_ecdc_death_cases_parameters_sinkFileName": {
						"value": "[parameters('tr_ingest_hospital_admission_data_properties_pl_ecdc_death_cases_parameters_sinkFileName')]"
					},
					"tr_transform_formula1_data_properties_pipeline_parameters_p_window_end_date": {
						"value": "[parameters('tr_transform_formula1_data_properties_pipeline_parameters_p_window_end_date')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'databrick-course-df_ArmTemplate_0')]"
			]
		}
	]
}